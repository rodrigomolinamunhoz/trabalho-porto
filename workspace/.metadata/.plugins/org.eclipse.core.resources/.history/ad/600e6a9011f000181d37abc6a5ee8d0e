package portoBackEnd;

public class Transportadora {
	private String nome;
	private Navio navio;
	private Caminhao caminhao;
	
	public Transportadora(String nome, Navio navio) {
		super();
		this.nome = nome;
		this.navio = navio;
	}

	public Transportadora(String nome, Caminhao caminhao) {
		super();
		this.nome = nome;
		this.caminhao = caminhao;
	}	
}

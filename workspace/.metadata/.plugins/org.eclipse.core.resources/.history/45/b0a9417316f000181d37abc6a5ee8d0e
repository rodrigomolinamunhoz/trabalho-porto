package portoBackEnd;

import java.util.ArrayList;

public class TesteBackEnd {

	public static void main(String[] args) {
	}
	
	private static Porto criarPorto() {
		return new Porto();
	}
	
	private static Porto criarNavios(Porto porto) {
		ArrayList<Navio> navios = new ArrayList<Navio>();
		navios.add(new Navio(111));
		navios.add(new Navio(222));
		navios.add(new Navio(333));
		porto.setNavios(navios);
		
		return porto;
	}
	
	private static Porto criarCaminhoes(Porto porto) {
		ArrayList<Caminhao> caminhoes = new ArrayList<Caminhao>();
		caminhoes.add(new Caminhao("AAA"));
		caminhoes.add(new Caminhao("BBB"));
		caminhoes.add(new Caminhao("CCC"));
		porto.setCaminhoes(caminhoes);
		
		return porto;
	}
}
